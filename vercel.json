{
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/widget/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/app/(.*)",
      "destination": "/app/dist/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/index.html",
      "statusCode": 200
    }
  ]
}
